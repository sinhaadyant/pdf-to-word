{
  "name": "pdf2docx-api",
  "version": "2.0.0",
  "description": "REST API for converting PDF to DOCX using pdf2docx with multi-file upload support",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf uploads/* outputs/*",
    "clean:all": "rm -rf uploads/* outputs/* node_modules package-lock.json",
    "setup": "npm install && mkdir -p uploads outputs",
    "health": "curl http://localhost:3000/health",
    "docs": "jsdoc -c jsdoc.json",
    "docker:build": "docker build -t pdf2docx-api .",
    "docker:run": "docker run -p 3000:3000 pdf2docx-api"
  },
  "keywords": [
    "pdf",
    "docx",
    "converter",
    "api",
    "rest",
    "express",
    "multer",
    "batch-processing",
    "file-upload"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/pdf2docx-api.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/pdf2docx-api/issues"
  },
  "homepage": "https://github.com/yourusername/pdf2docx-api#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jsdoc": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "node": true,
      "jest": true
    }
  }
}
